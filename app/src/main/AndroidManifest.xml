<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jp.sparkworks.restaurant">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.setting.settingActivities.SettingLotteryHistory"
            android:label="@string/setting_lottery_history_activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.setting.settingActivities.FeedbackActivity"
            android:label="@string/setting_feedback_activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.setting.settingActivities.LegalActivity"
            android:label="@string/setting_legal_activity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.setting.settingActivities.AppInfoActivity"
            android:label="@string/setting_app_info_activity"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>